<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Country"
            android:label="Country Music"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Jazz"
            android:label="Jazz Music"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Pop"
            android:label="Pop Music"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Instrumental"
            android:label="Instrumental Music"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".nowplaying"
            android:label="Playlist" />
    </application>

</manifest>